    window.showNewUser = true;

    
    (function(){
        if (window.localStorage && showNewUser){
            var c = parseInt(localStorage['nuCounter'],10); 
            c = isNaN(c) ? 1 : c+1;
            window.localStorage['nuCounter'] = c; 
            showNewUser = c < 10;
        }
    })();
    
</script>

