<div id="answer-5044" class="answer" data-answerid="5044"  itemprop="suggestedAnswer" itemscope itemtype="http://schema.org/Answer">
    <div class="post-layout">
            <div class="votecell post-layout--left">
                

<div class="js-voting-container grid fd-column ai-stretch gs4 fc-black-200" data-post-id="5044">
